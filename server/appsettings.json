{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  // Добавете конфигурация за JWT
  "Jwt": {
    "Key": "your-very-strong-secret-key-of-at-least-32-bytes",
    "Issuer": "YourIssuer",
    "Audience": "YourAudience"
  },
  {
    "Audience": "YourAudience",
    "RateLimiting": {
      "RequestLimit": 100,
      "TimeWindowInSeconds": 60
    }
  },
  "IpRateLimiting": {
    "EnableEndpointRateLimiting": true,
    "StackBlockedRequests": false,
    "RealIpHeader": "X-Real-IP",
    "ClientHeader": "X-ClientId",
    "QuotaExceededMessage": "Too many requests. Please try again later.",
    "GeneralRules": [
      {
        "Endpoint": "*",
        "Period": "10s",
        "Limit": 10
      },
      {
        "Endpoint": "api/user/*",
        "Period": "1m",
        "Limit": 20
      }
    ]
  }
}
  
